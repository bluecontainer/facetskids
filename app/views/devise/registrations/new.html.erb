
<%
  @path = registration_path(resource_name)
  @method = :post
%>

<% if @plan == "giftcard_purchase" %>
<%= render "devise/shared/registration_giftcard_purchase" %>
<% elsif @plan == "giftcard_redeem" %>
<%= render "devise/shared/registration_giftcard_redeem" %>
<% else %>
<%= render "devise/shared/registration" %>
<% end %>